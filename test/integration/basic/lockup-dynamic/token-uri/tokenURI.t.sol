pragma solidity >=0.8.19 <0.9.0;

import { Base64 } from "solady/utils/Base64.sol";

import { Dynamic_Integration_Basic_Test } from "../Dynamic.t.sol";

/// @dev Requirements for these tests to work:
/// - The stream id must be 1
/// - The stream sender must be `0x6332e7b1deb1f1a0b77b2bb18b144330c7291bca`, i.e. `makeAddr("Sender")`
/// - The stream asset must have the DAI symbol
/// - The contract deployer, i.e. the `sender` config option in `foundry.toml`, must have the default value
/// 0x1804c8AB1F12E6bbf3894d4083f33e07309d1f38 so that the deployed contracts have the same addresses as
/// the ones in the hard-coded values
contract TokenURI_Dynamic_Integration_Basic_Test is Dynamic_Integration_Basic_Test {
    uint256 internal defaultStreamId;

    function test_RevertWhen_NFTDoesNotExist() external {
        uint256 nullStreamId = 1729;
        vm.expectRevert("ERC721: invalid token ID");
        dynamic.tokenURI({ tokenId: nullStreamId });
    }

    modifier whenNFTExists() {
        defaultStreamId = createDefaultStream();
        vm.warp({ timestamp: defaults.START_TIME() + defaults.TOTAL_DURATION() / 4 });
        _;
    }

    function test_TokenURI_Decoded() external whenNFTExists {
        string memory tokenURI = dynamic.tokenURI(defaultStreamId);
        string memory actualDecodedTokenURI = string(Base64.decode(tokenURI));
        string memory expectedDecodedTokenURI =
            unicode'data:application/json;base64,{"attributes":[{"trait_type":"Asset","value":"DAI"},{"trait_type":"Sender","value":"0x6332e7b1deb1f1a0b77b2bb18b144330c7291bca"},{"trait_type":"Status","value":"Streaming"}],"description":"This NFT represents a payment stream in a Sablier V2 Lockup Dynamic contract. The owner of this NFT can withdraw the streamed assets, which are denominated in DAI.\\n\\n- Stream ID: 1\\n- Lockup Dynamic Address: 0xdb25a7b768311de128bbda7b8426c3f9c74f3240\\n- DAI Address: 0x03a6a84cd762d9707a21605b548aaab891562aab\\n\\n⚠️ WARNING: Transferring the NFT makes the new owner the recipient of the stream. The funds are not automatically withdrawn for the previous recipient.","external_url":"https://sablier.com","name":"Sablier V2 Lockup Dynamic #1","image":"data:image/svg+xml;base64,"}';
        assertEq(actualDecodedTokenURI, expectedDecodedTokenURI, "decoded token URI");
    }

    function test_TokenURI_Full() external whenNFTExists {
        string memory actualTokenURI = dynamic.tokenURI(defaultStreamId);
        string memory expectedTokenURI =
            "";
        assertEq(actualTokenURI, expectedTokenURI, "token URI");
    }
}
